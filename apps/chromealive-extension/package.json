{
  "name": "@ulixee/apps-chromealive-extension",
  "version": "2.0.0-alpha.2",
  "private": true,
  "license": "MIT",
  "description": "Chrome extension helps ChromeAlive work",
  "main": "index.js",
  "author": "Data Liberation Foundation",
  "scripts": {
    "serve": "vue-cli-service serve --watch",
    "build:webpack": "webpack --config webpack.config.js",
    "build:vue": "vue-cli-service build",
    "build": "yarn build:vue && yarn build:webpack",
    "build:dist": "cross-env BUILD_DIR=\"build-dist\" yarn build:vue && cross-env BUILD_DIR=\"build-dist\" yarn build:webpack",
    "watch": "concurrently -n vue,webpack \"vue-cli-service build --watch --mode=production\" \"webpack --watch --config webpack.config.js\""
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/runtime-corejs3": "^7.13.10",
    "@headlessui/vue": "^1.5.0",
    "@heroicons/vue": "^1.0.6",
    "@ulixee/apps-chromealive-core": "2.0.0-alpha.2",
    "@ulixee/apps-chromealive-interfaces": "2.0.0-alpha.2",
    "@ulixee/commons": "2.0.0-alpha.2",
    "@ulixee/hero-interfaces": "2.0.0-alpha.3",
    "@webcomponents/custom-elements": "^1.5.0",
    "core-js": "^3.21.1",
    "nanoid": "^3.1.30",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.0",
    "@types/chrome": "^0.0.154",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.1.0",
    "@vue/cli-plugin-babel": "~5.0.1",
    "@vue/cli-plugin-typescript": "~5.0.1",
    "@vue/cli-service": "~5.0.1",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0 ",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "chrome-api-mock": "^0.0.2",
    "concurrently": "^6.2.1",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "glob": "^7.1.6",
    "jest-chrome": "^0.7.2",
    "node-sass": "^6.0.1",
    "postcss": "^8.4.7",
    "prettier": "^2.2.1",
    "sass": "^1.43.4",
    "sass-loader": "^10.2.0",
    "tailwindcss": "^3.0.23",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.0"
  },
  "nohoist": [
    "babel-loader",
    "@vue/**",
    "**/@typescript-eslint",
    "**/@typescript-eslint/**",
    "**/vue",
    "**/vue-loader",
    "**/vue-template-compiler",
    "**/vue-class-component",
    "@achrinza/node-ipc",
    "**/postcss",
    "**/eslint/**"
  ]
}
