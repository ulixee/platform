{
  "name": "@ulixee/website",
  "version": "1.5.4",
  "private": true,
  "scripts": {
    "generate": "node ./data-generator/index.mjs",
    "start": "yarn generate && vue-cli-service serve --watch",
    "deploy": "firebase deploy --only hosting",
    "build": "yarn generate && vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "watch:stream": "npx onchange '../stream/docs/**/*.md' -- node ./data-generator/generateDoc.mjs {{file}}",
    "watch:databox": "npx onchange '../databox/docs/**/*.md' -- node ./data-generator/generateDoc.mjs {{file}}",
    "watch:hero": "npx onchange '../hero/docs/**/*.md' -- node ./data-generator/generateDoc.mjs {{file}}",
    "watch:server": "npx onchange '../server/docs/**/*.md' -- node ./data-generator/generateDoc.mjs {{file}}",
    "watch": "concurrently -n w: \"npm:watch:*\""
  },
  "dependencies": {
    "@headlessui/vue": "^1.5.0",
    "@heroicons/vue": "^1.0.6",
    "@tailwindcss/forms": "^0.5.0",
    "axios": "^0.26.1",
    "babel-plugin-prismjs": "^2.1.0",
    "core-js": "^3.21.1",
    "docsearch.js": "^2.6.3",
    "hastscript": "^7.0.2",
    "js-yaml": "^4.1.0",
    "concurrently": "^6.2.2",
    "lodash.kebabcase": "^4.1.1",
    "param-case": "^3.0.4",
    "prismjs": "^1.28.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.0.1",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-heading-id": "^1.0.0",
    "remark-html": "^15.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "sass": "^1.49.9",
    "unified": "^10.1.2",
    "unified-stream": "^2.0.0",
    "unist-util-visit": "^4.1.0",
    "vfile": "^5.3.2",
    "vue": "^3.2.31",
    "vue-inline-svg": "^3.1.0",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
    "@types/lodash.kebabcase": "^4.1.7",
    "@types/node": "^16.7.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~5.0.1",
    "@vue/cli-plugin-eslint": "~5.0.4",
    "@vue/cli-plugin-router": "~5.0.4",
    "@vue/cli-plugin-typescript": "~5.0.1",
    "@vue/cli-service": "~5.0.1",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^7.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^7.0.0",
    "lint-staged": "^9.5.0",
    "onchange": "^7.1.0",
    "postcss": "^8.4.7",
    "sass-loader": "^12.6.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.5.5"
  },
  "nohoist": [
    "**/@typescript-eslint",
    "**/@typescript-eslint/**",
    "**/@vue/**",
    "**/@vue/**/**",
    "**/vue",
    "**/vue-loader",
    "**/vue-template-compiler",
    "**/postcss"
  ]
}
